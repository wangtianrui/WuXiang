<?xml version="1.0" encoding="utf-8"?>
<wuxiang.miku.scorpio.wuxiang.view.ProgressLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/progressLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <wuxiang.miku.scorpio.wuxiang.view.SuperSwipeRefreshLayout
        android:id="@+id/swipe"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/rv_food_recyclerview"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </wuxiang.miku.scorpio.wuxiang.view.SuperSwipeRefreshLayout>

</wuxiang.miku.scorpio.wuxiang.view.ProgressLayout>
